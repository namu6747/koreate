	private int age;	// 나이
	private String gender;	// 성별
	private char joinYN;	// 회원 탈퇴 여부 'Y'
	private Date regdate;	// 회원 등록일
	private Date updatedate;	// 회원정보 수정 시간
	
	
	
	
	
	
	
	
	
}
